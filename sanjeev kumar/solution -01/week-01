#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        int n,max,min;
        cin>>n;
        int ar[n];
        for(int i=0;i<n;i++)
        {
            cin>>ar[i];
        }
        int n1;
        cin>>n1;
        int ar1[n1];
        for(int i=0;i<n1;i++)
        {
            cin>>ar1[i];
        }
        max=ar[0];
        for(int i=0;i<n;i++)
        {
            if(max<ar[i])
            {
                max=ar[i];
            }
        }
        min=ar1[0];
        for(int i=0;i<n1;i++)
        {
            if(min>ar1[i])
            {
                min=ar1[i];
            }
        }
        cout<<max*min<<endl;
        
    }
	//code
	return 0;
}
